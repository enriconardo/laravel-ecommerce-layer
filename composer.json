{
    "name": "enriconardo/laravel-ecommerce-layer",
    "description": "A layer of ecommerce functionalities for Laravel",
    "keywords": [
        "laravel",
        "ecommerce",
        "laravel-ecommerce-layer"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Enrico Nardo",
            "email": "hello@enriconardo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^9.0|^10.0",
        "stripe/stripe-php": "^10.7",
        "spatie/laravel-query-builder": "^5.2",
        "prinsfrank/standards": "^1.5"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "EnricoNardo\\EcommerceLayer\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EnricoNardo\\EcommerceLayer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "EnricoNardo\\EcommerceLayer\\Providers\\ServiceProvider",
                "EnricoNardo\\EcommerceLayer\\Providers\\RouteServiceProvider",
                "EnricoNardo\\EcommerceLayer\\Providers\\AuthServiceProvider",
                "EnricoNardo\\EcommerceLayer\\Providers\\GatewayServiceProvider"
            ]
        }
    }
}